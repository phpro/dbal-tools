{
    "name": "phpro/dbal-tools",
    "description": "Tools for working with DBAL in Symfony applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Phpro\\DbalTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhproTest\\DbalTools\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Toon Verwerft",
            "email": "toon.verwerft@phpro.be"
        }
    ],
    "require": {
        "php": "~8.4.0 || ~8.5.0",
        "azjezz/psl": "^3.2 || ^4.0",
        "doctrine/dbal": "^4.2",
        "symfony/config": "^7.3",
        "symfony/console": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/http-kernel": "^7.3",
        "symfony/property-access": "^7.3",
        "symfony/translation": "^7.3",
        "symfony/validator": "^7.3",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/doctrine-bundle": "^2.14 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~12.4.0",
        "brianium/paratest": "^7.14.1",
        "vimeo/psalm": "~6.13.0",
        "psalm/plugin-symfony": "^5.2",
        "php-standard-library/psalm-plugin": "^2.3",
        "php-cs-fixer/shim": "~3.88.0",
        "phpro/grumphp-shim": "^2.17",
        "amphp/dns": "^2.4",
        "amphp/socket": "^2.3.1",
        "ramsey/uuid": "^4.9"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp-shim": true
        }
    }
}
